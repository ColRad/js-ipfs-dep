{
  "name": "js-ipfs-dep",
  "version": "0.1.4",
  "description": "Compiles binary packages from js-ipfs",
  "repository": {
    "type": "git",
    "url": "https://github.com/ColRad/js-ipfs-dep.git"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
