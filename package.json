    "jsipfs": "node_modules/ipfs/src/cli/bin.js"
  },
  "pkg": {
    "scripts": "node_modules/ipfs/src/cli/commands/**/*.js"
  },
  "author": "ColRad",
  "license": "MIT",
  "dependencies": {
    "ipfs": "v0.35.0-pre.0",
    "pkg": "^4.3.7"
  }
